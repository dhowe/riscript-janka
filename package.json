{
  "name": "riscript",
  "version": "1.1.0",
  "description": "riscript",
  "type": "module",
  "main": "dist/index.cjs",
  "browser": "dist/index.iife.js",
  "exports": "./dist/index.js",
  "scripts": {
    "lint": "semistandard | snazzy",
    "lint:fix": "semistandard --fix | snazzy",
    "lint:test": "npm run lint && npm test",
    "test": "mocha \"test/**/*.tests.js\"",
    "test:coverage": "NODE_ENV=test c8 --all --include=lib/* --reporter=html --reporter=text mocha \"tests/**/*.tests.js\"",
    "test:watch": "npm test -- --watch",
    "docs": "jsdoc --readme README.md ./lib/*.js -d ./docs && jsdoc2md ./lib/*.js > ./api.md",
    "build": "rm -rf dist/* && rollup --config rollup.config.js"
  },
  "babel": {
    "compact": false,
    "presets": [
      "@babel/env"
    ],
    "env": {
      "production": {
        "compact": false
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "semistandard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "docs/",
      "node_modules/",
      "dist/"
    ]
  },
  "targets": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dhowe/riscript.git"
  },
  "keywords": [
    "npm",
    "package",
    "template",
    "javascript"
  ],
  "author": "Daniel Howe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhowe/riscript/issues"
  },
  "engines": {
    "node": ">= 14.x"
  },
  "files": [
    "dist",
    "API.md",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/dhowe/riscript#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "cross-env": "7.0.3",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "rollup": "^4.0.2",
    "semistandard": "^17.0.0",
    "snazzy": "^9.0.0"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "he": "^1.2.0",
    "mingo": "^6.4.7"
  }
}
